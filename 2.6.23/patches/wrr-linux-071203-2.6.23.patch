+
+		foo = *toupdate;
+
+		/* Delete it from the hashing list */
+		*toupdate = next[*toupdate];
+
+		/* And put it on the free list */
+		next[foo] = m->free_first;
+		m->free_first = foo;
+
+		m->cur_con--;
+	}
+
+	return 0;
