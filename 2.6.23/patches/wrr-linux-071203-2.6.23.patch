+static int wrr_tune_proxy(struct Qdisc *sch, struct rtattr *opt)
+{
+	struct wrr_sched_data *q = qdisc_priv(sch);
+	struct tc_wrr_qdisc_modf_proxy *qopt = RTA_DATA(opt);
+	int i;
